<div id="rejectwindow" class="textCenter inputButtons">
	<label>Remark</label>
	<textarea id="remark" name="remark" rows="5" cols="35"></textarea>
	<input type="submit" id="proceed" class="btn btn-primary"
		value="Proceed" /> <input type="button" id="cancel"
		class="btn btn-danger" value="Cancel" />
</div>
<div id="confirm_authorization" class="textCenter inputButtons">
	<h3>Do you want to proceed for Authorization?</h3>
	<input type="button" id="yes" class="btn btn-primary" value="Yes" /> <input
		type="button" id="no" class="btn btn-danger" value="No" />
</div>